{
  "name": "senior-cap-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
  "start": "concurrently \"npm run dev\" \"npm run start:frontend\"",
  "dev": "nodemon --watch './**/*.ts' --exec ts-node-esm --project tsconfig.server.json ./bin/www.ts",
  "start:backend": "ts-node ./bin/www",
  "start:frontend": "cd app_public/DigiP && npm start"
},
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "googleapis": "^105.0.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.1",
    "@types/logger": "^0.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.1",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
