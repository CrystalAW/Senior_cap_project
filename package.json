{
  "name": "senior-cap-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "postinstall": "npm install --save-dev ts-node typescript",
    "start": "concurrently \"npm run dev\" \"npm run start:python\" \"npm run start:frontend\"",
    "dev": "nodemon --watch './**/*.ts' --exec ts-node --project tsconfig.json src/bin/www.ts",
    "start:backend": "ts-node-esm ./src/bin/www",
    "start:frontend": "cd src/app_public/DigiP && npm start",
    "start:python": "python Algorithm/app.py",
    "start:node": "node dist/bin/www.js"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.18.1",
    "googleapis": "^105.0.0",
    "ts-node": "^10.9.2",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.48",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.0",
    "morgan": "~1.9.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/logger": "^0.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.3",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
